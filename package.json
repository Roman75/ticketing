{
  "name": "project_template_build_and_deploy",
  "version": "0.0.1",
  "author": "Roman Marlovits",
  "email": "roman.marlovits@gmail.com",
  "url": "http://www.webcomplete.at",
  "license": "ISC",
  "description": "build and deploy template for every upcomming project",
  "main": "index.js",
  "scripts": {
    "dist-start": "node dist/src/server.js",
    "dist-stop": "pkill --signal SIGINT ticketingserver",
    "forever-start": "./node_modules/.bin/forever -o ../../logs/forever_out.log -e ../../logs/forever_err.log start server.js",
    "forever-restart": "./node_modules/.bin/forever -o ../../logs/forever_out.log -e ../../logs/forever_err.log restart server.js || ./node_modules/.bin/forever -o ../../logs/forever_out.log -e ../../logs/forever_err.log start server.js",
    "forever-restartall": "./node_modules/.bin/forever restartall || ./node_modules/.bin/forever -o ../../logs/forever_out.log -e ../../logs/forever_err.log start server.js",
    "forever-stop": "./node_modules/.bin/forever stop server.js",
    "forever-stopall": "./node_modules/.bin/forever stopall",
    "forever-list": "./node_modules/.bin/forever list",
    "watch-server": "./node_modules/.bin/webpack --config webpack.config/server.js --env=watch",
    "watch-page": "./node_modules/.bin/webpack --config webpack.config/page.js --env=watch",
    "watch-admin": "./node_modules/.bin/webpack --config webpack.config/admin.js --env=watch",
    "watch-admin_mobile": "./node_modules/.bin/webpack --config webpack.config/admin_mobile.js --env=watch",
    "build-server": "./node_modules/.bin/webpack --config webpack.config/server.js --env=build",
    "build-page": "./node_modules/.bin/webpack --config webpack.config/page.js --env=build",
    "build-admin": "./node_modules/.bin/webpack --config webpack.config/admin.js --env=build",
    "build-admin_mobile": "./node_modules/.bin/webpack --config webpack.config/admin_mobile.js --env=build",
    "build-docs": "./node_modules/.bin/jsdoc --private -c ./jsdocs.json"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.appcomplete.at:8888/examples/project_template.git"
  },
  "keywords": [
    "project",
    "template",
    "server",
    "homepage"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdoc": "^3.5.5",
    "load-grunt-configs": "^1.0.0",
    "requirejs": "^2.3.5",
    "tui-jsdoc-template": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-ssh-deploy-plugin": "^0.2.0",
    "webpack-version-file": "^0.1.3"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "forever": "^0.15.3",
    "fs": "0.0.1-security",
    "ini": "^1.3.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "rm-log": "^1.2.6",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "tiny-emitter": "^2.0.2"
  }
}
